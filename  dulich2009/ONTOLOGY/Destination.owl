<?xml version="1.0"?>
<rdf:RDF
    xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns="http://www.owl-ontologies.com/Ontology1253893790.owl#"
  xml:base="http://www.owl-ontologies.com/Ontology1253893790.owl">
  <owl:Ontology rdf:about=""/>
  <owl:Class rdf:ID="AverageTemperature"/>
  <owl:Class rdf:ID="Climate"/>
  <owl:Class rdf:ID="City">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Destination"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Restriction>
            <owl:allValuesFrom>
              <owl:Class rdf:ID="Country"/>
            </owl:allValuesFrom>
            <owl:onProperty>
              <owl:InverseFunctionalProperty rdf:ID="isCityOf"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:InverseFunctionalProperty rdf:about="#isCityOf"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="DestinationCharacteristics"/>
  <owl:Class rdf:ID="PeriodOfTime">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Period"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Country">
    <rdfs:subClassOf rdf:resource="#Destination"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasCity"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Restriction>
            <owl:someValuesFrom rdf:resource="#City"/>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#hasCity"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="hasRegion"/>
            </owl:onProperty>
            <owl:someValuesFrom>
              <owl:Class rdf:ID="Region"/>
            </owl:someValuesFrom>
          </owl:Restriction>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasCity"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Activity">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >enables  the  representation  of  activities,  which  are  of  relevance  for  a  journey  or 
business trip</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="RegionCharacteristics">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >represents basic characteristics of a region.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#DestinationCharacteristics"/>
  </owl:Class>
  <owl:Class rdf:ID="PointOfTime"/>
  <owl:Class rdf:ID="CityCharacteristics">
    <rdfs:subClassOf rdf:resource="#DestinationCharacteristics"/>
  </owl:Class>
  <owl:Class rdf:ID="CountryCharacteristics">
    <rdfs:subClassOf rdf:resource="#DestinationCharacteristics"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >represents basic characteristics of a country</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="MonthTemperature"/>
  <owl:Class rdf:about="#Region">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Country"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="isRegionOf"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="#Destination"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="#isRegionOf">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <owl:inverseOf rdf:resource="#isRegionOf"/>
    <rdfs:range rdf:resource="#Country"/>
    <rdfs:domain rdf:resource="#Region"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasAverageTemperature">
    <rdfs:range rdf:resource="#AverageTemperature"/>
    <rdfs:domain rdf:resource="#Climate"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasPeriodOfTime">
    <rdfs:range rdf:resource="#PeriodOfTime"/>
    <rdfs:domain rdf:resource="#Activity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasActivity">
    <rdfs:domain rdf:resource="#Destination"/>
    <rdfs:range rdf:resource="#Activity"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasRegion">
    <rdfs:domain rdf:resource="#Country"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
    <owl:inverseOf rdf:resource="#hasRegion"/>
    <rdfs:range rdf:resource="#Region"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasDestinationCharacteristics">
    <rdfs:domain rdf:resource="#Destination"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="#DestinationCharacteristics"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasClimate">
    <rdfs:range rdf:resource="#Climate"/>
    <rdfs:domain rdf:resource="#DestinationCharacteristics"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="inverse_of_hasCity"/>
  <owl:ObjectProperty rdf:about="#hasCity">
    <rdfs:domain rdf:resource="#Country"/>
    <rdfs:range rdf:resource="#City"/>
    <owl:inverseOf rdf:resource="#hasCity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasMonthTemperature">
    <rdfs:range rdf:resource="#MonthTemperature"/>
    <rdfs:domain rdf:resource="#AverageTemperature"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="duration">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain rdf:resource="#Period"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="country">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >name of country</rdfs:comment>
    <rdfs:domain rdf:resource="#CountryCharacteristics"/>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >VietNam</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >England</rdf:first>
            <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
          </rdf:rest>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="temperature">
    <rdfs:domain rdf:resource="#MonthTemperature"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="language">
    <rdfs:domain rdf:resource="#DestinationCharacteristics"/>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >English</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Vietnamese</rdf:first>
            <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
          </rdf:rest>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="month">
    <rdfs:domain rdf:resource="#MonthTemperature"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="region">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >specify a region by its name (as a character string).</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#RegionCharacteristics"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="city">
    <rdfs:domain rdf:resource="#CityCharacteristics"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="climateCategory">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >specify a category for a climate (tropical, sub-tropical,...).</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Climate"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="altitude">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#CityCharacteristics"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >specify the altitude (độ cao so với mặt nước biển) of a city.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="landscape">
    <rdfs:domain rdf:resource="#DestinationCharacteristics"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >specify the characteristic of the landscape (e.g. flat, hilly, mountainous,etc.).</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:FunctionalProperty rdf:ID="hasEnd">
    <rdfs:domain rdf:resource="#Period"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="#PointOfTime"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="date">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#PointOfTime"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="activityType">
    <rdfs:domain rdf:resource="#Activity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasBegin">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="#PointOfTime"/>
    <rdfs:domain rdf:resource="#Period"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="time">
    <rdfs:domain rdf:resource="#PointOfTime"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#time"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:InverseFunctionalProperty rdf:about="#isCityOf">
    <owl:inverseOf rdf:resource="#isCityOf"/>
    <rdfs:domain rdf:resource="#City"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
    <rdfs:range rdf:resource="#Country"/>
  </owl:InverseFunctionalProperty>
  <Country rdf:ID="Britain"/>
  <Region rdf:ID="Midland">
    <isRegionOf>
      <Country rdf:ID="VienNam">
        <isCityOf>
          <City rdf:ID="HaiPhong">
            <isCityOf rdf:resource="#VienNam"/>
          </City>
        </isCityOf>
        <hasCity>
          <City rdf:ID="KhanhHoa">
            <isCityOf rdf:resource="#VienNam"/>
            <hasCity rdf:resource="#VienNam"/>
          </City>
        </hasCity>
      </Country>
    </isRegionOf>
  </Region>
  <City rdf:ID="NewYork">
    <hasCity>
      <Country rdf:ID="USA">
        <hasCity rdf:resource="#NewYork"/>
        <isCityOf rdf:resource="#NewYork"/>
      </Country>
    </hasCity>
    <isCityOf rdf:resource="#USA"/>
  </City>
</rdf:RDF>

