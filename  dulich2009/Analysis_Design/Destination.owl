<?xml version="1.0"?>
<rdf:RDF
    xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns="http://www.owl-ontologies.com/Ontology1253893790.owl#"
  xml:base="http://www.owl-ontologies.com/Ontology1253893790.owl">
  <owl:Ontology rdf:about=""/>
  <owl:Class rdf:ID="Climate"/>
  <owl:Class rdf:ID="AverageTemperature"/>
  <owl:Class rdf:ID="DestinationCharacteristics"/>
  <owl:Class rdf:ID="City">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Destination"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="PeriodOfTime">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Period"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Country">
    <rdfs:subClassOf rdf:resource="#Destination"/>
  </owl:Class>
  <owl:Class rdf:ID="Activity"/>
  <owl:Class rdf:ID="RegionCharacteristics">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >represents basic characteristics of a region.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#DestinationCharacteristics"/>
  </owl:Class>
  <owl:Class rdf:ID="CityCharacteristics">
    <rdfs:subClassOf rdf:resource="#DestinationCharacteristics"/>
  </owl:Class>
  <owl:Class rdf:ID="CountryCharacteristics">
    <rdfs:subClassOf rdf:resource="#DestinationCharacteristics"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >represents basic characteristics of a country</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="MonthTemperature"/>
  <owl:Class rdf:ID="Region">
    <rdfs:subClassOf rdf:resource="#Destination"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="hasAverageTemperature">
    <rdfs:range rdf:resource="#AverageTemperature"/>
    <rdfs:domain rdf:resource="#Climate"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasPeriodOfTime">
    <rdfs:range rdf:resource="#PeriodOfTime"/>
    <rdfs:domain rdf:resource="#Activity"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasActivity">
    <rdfs:domain rdf:resource="#Destination"/>
    <rdfs:range rdf:resource="#Activity"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasDestinationCharacteristics">
    <rdfs:domain rdf:resource="#Destination"/>
    <rdfs:range rdf:resource="#DestinationCharacteristics"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasClimate">
    <rdfs:range rdf:resource="#Climate"/>
    <rdfs:domain rdf:resource="#DestinationCharacteristics"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasMonthTemperature">
    <rdfs:range rdf:resource="#MonthTemperature"/>
    <rdfs:domain rdf:resource="#AverageTemperature"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="duration">
    <rdfs:domain rdf:resource="#Period"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="activityType">
    <rdfs:domain rdf:resource="#Activity"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="country">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >name of country</rdfs:comment>
    <rdfs:domain rdf:resource="#CountryCharacteristics"/>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >VietNam</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >England</rdf:first>
          </rdf:rest>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="temperature">
    <rdfs:domain rdf:resource="#MonthTemperature"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="language">
    <rdfs:domain rdf:resource="#DestinationCharacteristics"/>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >English</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Vietnamese</rdf:first>
            <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
          </rdf:rest>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="month">
    <rdfs:domain rdf:resource="#MonthTemperature"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="region">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >specify a region by its name (as a character string).</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#RegionCharacteristics"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="city">
    <rdfs:domain rdf:resource="#CityCharacteristics"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="climateCategory">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >specify a category for a climate (tropical, sub-tropical,...).</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Climate"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="altitude">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#CityCharacteristics"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >specify the altitude (độ cao so với mặt nước biển) of a city.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="landscape">
    <rdfs:domain rdf:resource="#DestinationCharacteristics"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >specify the characteristic of the landscape (e.g. flat, hilly, mountainous,etc.).</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
</rdf:RDF>

