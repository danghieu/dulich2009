<?xml version="1.0"?>
<rdf:RDF
    xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns="http://www.owl-ontologies.com/Flight.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:sqwrl="http://sqwrl.stanford.edu/ontologies/built-ins/3.4/sqwrl.owl#"
  xml:base="http://www.owl-ontologies.com/Flight.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://sqwrl.stanford.edu/ontologies/built-ins/3.4/sqwrl.owl"/>
    <owl:imports rdf:resource="http://swrl.stanford.edu/ontologies/3.3/swrla.owl"/>
  </owl:Ontology>
  <owl:Class rdf:ID="Msg_FlightBookChangeRQ"/>
  <owl:Class rdf:ID="Msg_FlightBookChangeRS"/>
  <owl:Class rdf:ID="Msg_FlightAvailRQ"/>
  <owl:Class rdf:ID="F_Flight">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="F_Price"/>
  <owl:Class rdf:ID="F_Airport"/>
  <owl:Class rdf:ID="F_PointOfTime"/>
  <owl:Class rdf:ID="Msg_FlightBookRQ"/>
  <owl:Class rdf:ID="F_Customer"/>
  <owl:Class rdf:ID="F_SecurityCode"/>
  <owl:Class rdf:ID="Msg_FlightBookChangeConfirmRQ"/>
  <owl:Class rdf:ID="Msg_FlightBookRS"/>
  <owl:Class rdf:ID="Msg_FlightAvailRS"/>
  <owl:Class rdf:ID="F_Airplane"/>
  <owl:Class rdf:ID="F_Adress"/>
  <owl:ObjectProperty rdf:ID="hasCustomer">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Msg_FlightBookRQ"/>
          <owl:Class rdf:about="#Msg_FlightBookRS"/>
          <owl:Class rdf:about="#Msg_FlightBookChangeRS"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasMsg_FlightAvaiRS">
    <rdfs:range rdf:resource="#Msg_FlightAvailRS"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Msg_FlightBookRQ"/>
          <owl:Class rdf:about="#Msg_FlightBookRS"/>
          <owl:Class rdf:about="#Msg_FlightBookChangeRS"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasDepartureDateTime">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#F_Flight"/>
          <owl:Class rdf:about="#Msg_FlightAvailRQ"/>
          <owl:Class rdf:about="#Msg_FlightAvailRS"/>
          <owl:Class rdf:about="#Msg_FlightBookChangeRQ"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="#F_PointOfTime"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasAdress">
    <rdfs:domain rdf:resource="#F_Airport"/>
    <rdfs:range rdf:resource="#F_Adress"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasArrivalAirport">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#F_Flight"/>
          <owl:Class rdf:about="#Msg_FlightAvailRQ"/>
          <owl:Class rdf:about="#Msg_FlightAvailRS"/>
          <owl:Class rdf:about="#Msg_FlightBookChangeRQ"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="#F_Airport"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasPrice">
    <rdfs:range rdf:resource="#F_Price"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#F_Flight"/>
          <owl:Class rdf:about="#Msg_FlightAvailRS"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasArrivalDateTime">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#F_Flight"/>
          <owl:Class rdf:about="#Msg_FlightAvailRQ"/>
          <owl:Class rdf:about="#Msg_FlightAvailRS"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="#F_PointOfTime"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasSecurityCode">
    <rdfs:range rdf:resource="#F_SecurityCode"/>
    <rdfs:domain rdf:resource="#Msg_FlightBookRS"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasAirplane">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#F_Flight"/>
          <owl:Class rdf:about="#Msg_FlightAvailRS"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="#F_Airplane"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasMsg_FlightBookChangeRS">
    <rdfs:domain rdf:resource="#Msg_FlightBookChangeConfirmRQ"/>
    <rdfs:range rdf:resource="#Msg_FlightBookChangeRS"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasDepartureAirport">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#F_Flight"/>
          <owl:Class rdf:about="#Msg_FlightAvailRQ"/>
          <owl:Class rdf:about="#Msg_FlightAvailRS"/>
          <owl:Class rdf:about="#Msg_FlightBookChangeRQ"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="#F_Airport"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="numberOfStop">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >So diem dung may bay</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="city">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain rdf:resource="#F_Adress"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="price">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#F_Price"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="phoneNumber">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#F_Customer"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="name">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#F_Airport"/>
          <owl:Class rdf:about="#Msg_FlightBookChangeRQ"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="airplaneMaker">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Hang san xuat may bay (Airbus, Boeing)</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#F_Airplane"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="airline">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#F_Flight"/>
          <owl:Class rdf:about="#Msg_FlightAvailRS"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Hang hang khong</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="email">
    <rdfs:domain rdf:resource="#F_Customer"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="notification">
    <rdfs:domain rdf:resource="#Msg_FlightBookRS"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Thong bao kem theo</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="flightClass">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#F_Flight"/>
          <owl:Class rdf:about="#Msg_FlightAvailRQ"/>
          <owl:Class rdf:about="#Msg_FlightAvailRS"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >business</rdf:first>
            <rdf:rest rdf:parseType="Resource">
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >economy</rdf:first>
              <rdf:rest rdf:parseType="Resource">
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >premium economy</rdf:first>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:rest>
            </rdf:rest>
          </rdf:rest>
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >firstClass</rdf:first>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Loai ve may bay</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="desciption">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#F_Flight"/>
          <owl:Class rdf:about="#Msg_FlightAvailRS"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="password">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#F_SecurityCode"/>
          <owl:Class rdf:about="#Msg_FlightBookChangeConfirmRQ"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="fullName">
    <rdfs:domain rdf:resource="#F_Customer"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="flightNumber">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#F_Flight"/>
          <owl:Class rdf:about="#Msg_FlightAvailRS"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Ma so chuyen bay</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="avaibleNumber">
    <rdfs:domain rdf:resource="#F_Flight"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="time">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Thoi gian cu the trong ngay</rdfs:comment>
    <rdfs:domain rdf:resource="#F_PointOfTime"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#time"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="airplaneType">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain rdf:resource="#F_Airplane"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="gender">
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >male</rdf:first>
            <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
          </rdf:rest>
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >female</rdf:first>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
    <rdfs:domain rdf:resource="#F_Customer"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="country">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#F_Adress"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="date">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Ngay cu the</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <rdfs:domain rdf:resource="#F_PointOfTime"/>
  </owl:DatatypeProperty>
  <owl:FunctionalProperty rdf:ID="priceUnit">
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >USD</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >VND</rdf:first>
          </rdf:rest>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
    <rdfs:domain rdf:resource="#F_Price"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="contractCode">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Ma hop dong</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#F_SecurityCode"/>
          <owl:Class rdf:about="#Msg_FlightBookChangeRQ"/>
          <owl:Class rdf:about="#Msg_FlightBookChangeRS"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:DataRange>
    <owl:oneOf rdf:parseType="Resource">
      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
      >Adults(&gt;=12)</rdf:first>
      <rdf:rest rdf:parseType="Resource">
        <rdf:rest rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Infant(0-2)</rdf:first>
          <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
        </rdf:rest>
        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >Children (2-11)</rdf:first>
      </rdf:rest>
    </owl:oneOf>
  </owl:DataRange>
  <owl:DataRange>
    <owl:oneOf rdf:parseType="Resource">
      <rdf:rest rdf:parseType="Resource">
        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >direct</rdf:first>
        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
      </rdf:rest>
      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
      >nonstop</rdf:first>
    </owl:oneOf>
  </owl:DataRange>
</rdf:RDF>

